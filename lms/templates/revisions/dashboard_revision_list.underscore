<span class="title edit-name">
    <% if (totalCount > 0 ) { %>
        <%= interpolate(
            ngettext(
                "You have (%(num_items)s) pending revision:",
                "You have (%(num_items)s) pending revisions:",
                totalCount
            ),
            { num_items: totalCount },
            true
        ) %>
    <% } else { %>
        <%= gettext("You have no pending revisions.") %>
    <% } %>
</span>
<ul class="revision-list"></ul>
